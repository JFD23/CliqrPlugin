{{#> mc_header}}
{{#*inline "title"}} Multiple-Choice-Frage: {{/inline}}
{{/mc_header}}

<main>
    <table class="cliqr--multiple-choice-answers">
        <tbody>
            {{#each answers}}
                <tr>
                    <td class="nominal"> {{nominal @index}} </td>
                    <td class="text"> {{text}} </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</main>
